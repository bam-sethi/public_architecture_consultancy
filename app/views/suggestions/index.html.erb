<ul>
<% @suggestions.each do |suggestion| %>


  <li data-building-id="<%= suggestion.building.id %>" data-suggestion-id="<%= suggestion.id %>"><%= image_tag(@building.image, class: "building-like-image") %><%= suggestion.suggestion_content %> | <%= image_tag("like.png", class: "like-button") %> | <span class="likes"><%= suggestion.number_of_likes %></span></li>

<% end %>

</ul>

<!-- This links the like.js file for JUST this page -->
<%= javascript_include_tag 'like' %>




